{
    "version": 2,
    "buids": [
        {
            "src": "playlistdownloaer/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": { "destDir": "staticfiles_build"}
        },
        {
            "routes": [
                {
                    "src":"/static/(.*)",
                    "dest" : "/static/$1"
                },
                {
                    "src":"/(.*)",
                    "dest": "playlistdownloaer/wsgi.py"
                }
        
        ]
        }
    ]
}